{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "carbon-blocks/card",
  "version": "1.0.0",
  "category": "carbon-blocks",
  "supports": {
    "html": false,
    "anchor": true,
    "customClassName": true,
    "multiple": true,
    "reusable": true,
    "align": false,
    "color": {
      "link": false,
      "text": false,
      "gradients": true,
      "background": false,
      "enableContrastChecker": false,
      "__experimentalDefaultControls": {}
    },
    "spacing": {
      "blockGap": {
        "default": "10px",
        "__experimentalDefault": "10px",
        "sides": [
          "vertical"
        ]
      },
      "margin": false,
      "padding": true,
      "__experimentalDefaultControls": {
        "margin": false,
        "padding": true,
        "blockGap": true
      }
    },
    "layout": {
      "allowSwitching": false,
      "allowEditing": true,
      "allowInheriting": false,
      "allowSizingOnChildren": false,
      "allowVerticalAlignment": false,
      "allowJustification": false,
      "allowOrientation": false,
      "default": {
        "type": "flex",
        "orientation": "vertical",
        "justifyContent": "left",
        "flexWrap": "nowrap"
      }
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      }
    },
    "typography": {
      "fontSize": false,
      "lineHeight": false,
      "__experimentalFontFamily": false,
      "__experimentalFontWeight": false,
      "__experimentalFontStyle": false,
      "__experimentalTextTransform": false,
      "__experimentalTextDecoration": false,
      "__experimentalLetterSpacing": false,
      "__experimentalDefaultControls": {
        "fontSize": false,
        "FontFamily": false
      }
    }
  },
  "attributes": {
    "showActions": {
      "type": "boolean",
      "default": true
    },
    "seeMore": {
      "type": "string",
      "default": "",
      "selector": "span"
    },
    "post": {
      "type": "object"
    },
    "linkColor": {
      "type": "string"
    },
    "backgroundColor": {
      "type": "string"
    },
    "showSeeMore": {
      "type": "boolean",
      "default": true
    },
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "blockGap": "10px"
        }
      }
    }
  },
  "providesContext": {},
  "usesContext": [],
  "textdomain": "carbon-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}