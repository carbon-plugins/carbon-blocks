{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "carbon-blocks/divider",
	"version": "1.0.0",
	"category": "carbon-blocks",
	"supports": {
		"html": false,
    "anchor": true,
		"customClassName": true,
		"multiple": true,
		"reusable": true,
		"align": [ "wide", "full" ],
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"__experimentalDefaultControls": {
				"margin": true,
				"padding": true,
				"blockGap": false
			}
		},
		"layout": {
			"allowSwitching": false,
			"allowInheriting": false,
			"allowEditing": false,
			"default": {
				"type": "flex"
			}
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"style": true,
				"width": true
			}
		},
    "typography": {
      "fontSize": false,
      "lineHeight": false,
			"__experimentalFontFamily": false,
      "__experimentalFontWeight": false,
      "__experimentalFontStyle": false,
      "__experimentalTextTransform": false,
			"__experimentalTextDecoration": false,
      "__experimentalLetterSpacing": false,
      "__experimentalDefaultControls": {
        "fontSize": false,
        "FontFamily": false
      }
    }
	},
	"attributes": {
		"showActions": {
			"type": "boolean",
			"default": true
		},
		"isFlippedHorizontally": {
			"type": "boolean",
			"default": false
		},
		"isFlippedVertically": {
			"type": "boolean",
			"default": false
		},
    "align": {
      "type": "string",
      "default": "full"
    },
    "shapeType": {
      "type": "string",
      "default": "wave"
    },
		"backgroundColor": {
			"type": "string",
			"default": "#FFFFFF"
		},
		"dividerColor": {
			"type": "string",
			"default": "#E1E1E1"
		},
		"dividerHeight": {
			"type": "number",
			"default": 110
		}
	},
	"usesContext": [],
	"providesContext": {},
	"textdomain": "carbon-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
